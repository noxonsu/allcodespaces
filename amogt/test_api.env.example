# Environment variables for Partner API Test Suite
# Copy this file to .env.local and set your actual values
# DO NOT commit .env.local to version control!

# Required: Your partner API token
TEST_API_PARTNER_KEY=your_test_token_here

# Optional: API URL (defaults to localhost)
TEST_API_URL=http://localhost/allcodespaces/chatgptbot_connector/gpt_payment_api/api.php

# Example usage:
# export TEST_API_PARTNER_KEY=test_partner_token_123
# export TEST_API_URL=https://your-domain.com/api.php
# php test_partner_api.php
